function clk()  {
    pins.digitalWritePin(DigitalPin.P0, 0)
    pins.digitalWritePin(DigitalPin.P0, 1)
    pins.digitalWritePin(DigitalPin.P0, 0)
}
function latch()  {
    pins.digitalWritePin(DigitalPin.P2, 0)
    pins.digitalWritePin(DigitalPin.P2, 1)
    pins.digitalWritePin(DigitalPin.P2, 0)
}
function TurnR()  {
    AllStop()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 1)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    latch()
}
function AllStop()  {
    pins.digitalWritePin(DigitalPin.P0, 0)
    pins.digitalWritePin(DigitalPin.P1, 0)
    pins.digitalWritePin(DigitalPin.P2, 0)
    for (let i = 0; i <= 7; i++) {
        clk()
    }
    latch()
}
function TurnL()  {
    AllStop()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 1)
    clk()
    latch()
}
function AllFor()  {
    AllStop()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 1)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 1)
    clk()
    latch()
}
function AllRev()  {
    AllStop()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 1)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 1)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    latch()
}
function SpinCW()  {
    AllStop()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 1)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 1)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    latch()
}
function SpinACW()  {
    AllStop()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 1)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 0)
    clk()
    pins.digitalWritePin(DigitalPin.P1, 1)
    clk()
    latch()
}
